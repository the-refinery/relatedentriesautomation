{
    "name": "therefinery/relatedentriesautomation",
    "description": "Provides a field specify selection criteria for providing related entries",
    "type": "craft-plugin",
    "version": "0.3.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "relatedentriesautomation"
    ],
    "support": {
        "docs": "https://github.com/the-refinery/relatedentriesautomation",
        "issues": "https://github.com/the-refinery/relatedentriesautomation/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "The Refinery",
            "homepage": "http://the-refinery.io/"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
          "therefinery\\relatedentriesautomation\\": "src/"
        }
    },
    "extra": {
        "name": "relatedentriesautomation",
        "handle": "relatedentriesautomation",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://github.com/the-refinery/relatedentriesautomation/blob/master/CHANGELOG.md",
        "components": {
            "entriesFilterService": "therefinery\\relatedentriesautomation\\services\\EntriesFilterService",
            "entriesinfoService": "therefinery\\relatedentriesautomation\\services\\EntriesinfoService"
        },
        "class": "therefinery\\relatedentriesautomation\\Relatedentriesautomation"
    }
}
